{
  "name": "media-processing-pipeline",
  "version": "1.0.0",
  "description": "Scalable media file processing system",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run lint:check && npm run typecheck && tsc",
    "build:fast": "tsc",
    "dev": "npm run build:fast && node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts --max-warnings 25",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "etl:recreation": "npm run build:fast && node dist/scripts/etl-recreation-facilities.js",
    "etl:recreation:incremental": "npm run build:fast && node dist/scripts/etl-recreation-facilities.js --incremental",
    "mcp:start": "cd mcp-photo-search && ./venv/bin/python mcp_server.py",
    "mcp:start-simple": "cd mcp-photo-search && ./venv/bin/python mcp_server_simple.py",
    "mcp:test": "cd mcp-photo-search && ./venv/bin/python -c \"from embeddings.retrieval_engine import RetrievalEngine; engine = RetrievalEngine('embeddings.db'); print(engine.search('test query'))\"",
    "mcp:inject": "cd mcp-photo-search && ./venv/bin/python claude_brain.py ingest /photos",
    "mcp:rebuild": "cd mcp-photo-search && ./venv/bin/python claude_brain.py ingest /photos",
    "mcp:index-metadata": "cd mcp-photo-search && ./venv/bin/python claude_brain.py ingest /photos/metadata",
    "mcp:index-archive": "cd mcp-photo-search && ./venv/bin/python claude_brain.py ingest /photos/archive",
    "mcp:check-drift": "cd mcp-photo-search && ./venv/bin/python drift_monitor.py --check",
    "mcp:update-cache": "cd mcp-photo-search && ./venv/bin/python drift_monitor.py --update-cache",
    "mcp:auto-update": "cd mcp-photo-search && ./venv/bin/python drift_monitor.py --auto-update",
    "mcp:monitor": "cd mcp-photo-search && ./venv/bin/python schedule_monitor.py",
    "mcp:monitor-once": "cd mcp-photo-search && ./venv/bin/python schedule_monitor.py --once",
    "mcp:cleanup-db": "cd mcp-photo-search && ./venv/bin/python cleanup_database.py",
    "mcp:setup": "cd mcp-photo-search && ./setup.sh",
    "mcp-project:start": "cd mcp-project-context && ./venv/bin/python mcp_server_simple.py",
    "mcp-project:rebuild": "cd mcp-project-context && ./venv/bin/python claude_brain.py ingest /home/stephen/Documents/initial-media-processing",
    "mcp-project:check-drift": "cd mcp-project-context && ./venv/bin/python drift_monitor.py --check",
    "mcp-project:auto-update": "cd mcp-project-context && ./venv/bin/python drift_monitor.py --auto-update",
    "mcp-project:cleanup-db": "cd mcp-project-context && ./venv/bin/python cleanup_database.py",
    "mcp-project:test": "cd mcp-project-context && ./venv/bin/python -c \"from embeddings.retrieval_engine import RetrievalEngine; engine = RetrievalEngine('project-embeddings.db'); print(engine.search('CLI implementation'))\""
  },
  "keywords": [
    "media",
    "processing",
    "metadata",
    "exif"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "mysql2": "^3.14.1",
    "shapefile": "^0.6.6",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^30.0.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
